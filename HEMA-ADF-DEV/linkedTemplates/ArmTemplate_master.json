{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "HEMA-ADF-DEV"
		},
		"LS_SOURCE_DB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SOURCE_DB'"
		},
		"LS_TARGET_DB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_TARGET_DB'"
		},
		"ls_databricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_databricks'"
		},
		"LS_SOURCE_DB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserver-hema-dev.database.windows.net"
		},
		"LS_SOURCE_DB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Source_db"
		},
		"LS_SOURCE_DB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Hema"
		},
		"LS_TARGET_DB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserver-hema-dev.database.windows.net"
		},
		"LS_TARGET_DB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Target_db"
		},
		"LS_TARGET_DB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Hema"
		},
		"ls_databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1202-091041-qp8b2oe5"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "HEMA-ADF-DEV_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_SOURCE_DB_password": {
						"value": "[parameters('LS_SOURCE_DB_password')]"
					},
					"LS_TARGET_DB_password": {
						"value": "[parameters('LS_TARGET_DB_password')]"
					},
					"ls_databricks_accessToken": {
						"value": "[parameters('ls_databricks_accessToken')]"
					},
					"LS_SOURCE_DB_properties_typeProperties_server": {
						"value": "[parameters('LS_SOURCE_DB_properties_typeProperties_server')]"
					},
					"LS_SOURCE_DB_properties_typeProperties_database": {
						"value": "[parameters('LS_SOURCE_DB_properties_typeProperties_database')]"
					},
					"LS_SOURCE_DB_properties_typeProperties_userName": {
						"value": "[parameters('LS_SOURCE_DB_properties_typeProperties_userName')]"
					},
					"LS_TARGET_DB_properties_typeProperties_server": {
						"value": "[parameters('LS_TARGET_DB_properties_typeProperties_server')]"
					},
					"LS_TARGET_DB_properties_typeProperties_database": {
						"value": "[parameters('LS_TARGET_DB_properties_typeProperties_database')]"
					},
					"LS_TARGET_DB_properties_typeProperties_userName": {
						"value": "[parameters('LS_TARGET_DB_properties_typeProperties_userName')]"
					},
					"ls_databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}